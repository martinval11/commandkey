{
  "name": "commandkey",
  "description": "A simple command palette for React",
  "version": "0.2.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:library": "tsup"
  },
  "dependencies": {
    "commandkey": "^0.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.12"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "commandkey",
    "key",
    "keybinding",
    "popup",
    "search"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martinval11/commandkey/issues"
  },
  "homepage": "https://commandkey.vercel.app/"
}
